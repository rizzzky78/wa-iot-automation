{
  "compilerOptions": {
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@libs/*": ["./libs/*"],
      "@store": ["./store/index.js"],
      "@settings/*": ["./settings/*"],
      "@database/*": ["./database/*"],
      "@functions/*": ["./functions/*"],
      "@controllers/*": ["./controllers/*"],
      "@interfaces/*": ["./interfaces/*"]
    }
  },
  "exclude": ["node_modules"]
}
